      <!-- ! FORM INPUT -->
      <div id="form" class="vh bg-success-subtle">
        <div class="container bg-info inp-1">
          <h1 class="text-center">barang terjual</h1>
          <p>silahkan isi data barang : terjual</p>
          <div class="input-group input-group-lg mb-3">
            <span class="input-group-text" id="inputGroup-sizing-sm"
              >kode Barang</span
            >
            <input
              type="text"
              class="form-control"
              aria-label="Sizing example input"
              aria-describedby="inputGroup-sizing-sm"
              v-model="kode"
            />
          </div>
          <div class="input-group input-group-lg mb-3">
            <span class="input-group-text" id="inputGroup-sizing-default"
              >nama barang</span
            >
            <input
              type="text"
              class="form-control"
              aria-label="Sizing example input"
              aria-describedby="inputGroup-sizing-default"
              v-model="nama"
            />
          </div>
          <div class="input-group input-group-lg mb-3">
            <span class="input-group-text" id="inputGroup-sizing-lg"
              >jumlah barang terjual</span
            >
            <input
              type="text"
              class="form-control"
              aria-label="Sizing example input"
              aria-describedby="inputGroup-sizing-lg"
              v-model="harga"
            />
          </div>
          <div class="input-group input-group-lg mb-3">
            <span class="input-group-text" id="inputGroup-sizing-sm"
              >jumlah harga terjual</span
            >
            <input
              type="text"
              class="form-control"
              aria-label="Sizing example input"
              aria-describedby="inputGroup-sizing-sm"
              v-model="jumlah"
            />
          </div>
          <div class="input-group input-group-lg mb-3">
            <span class="input-group-text" id="inputGroup-sizing-sm"
              >tanggal</span
            >
            <input
              type="text"
              class="form-control"
              aria-label="Sizing example input"
              aria-describedby="inputGroup-sizing-sm"
              v-model="totalBeli"
            />
          </div>
          <button type="submit" class="btn btn-primary me-3">Submit</button>
          <button type="submit" class="btn btn-primary">cancel</button>
        </div>
      </div>
      <!-- ! FORM INPUT END -->
