<body>
  <div id="app" class="bg-success-subtle">
    <!--! NAVBAR -->
    <nav class="navbar navbar-expand-lg bg-body-tertiary">
      <div class="container-fluid bg-info c-2">
        <a class="navbar-brand logo" href="#">TokoAfiyah</a>
        <button
          class="navbar-toggler"
          type="button"
          data-bs-toggle="collapse"
          data-bs-target="#navbarTogglerDemo02"
          aria-controls="navbarTogglerDemo02"
          aria-expanded="false"
          aria-label="Toggle navigation"
        >
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarTogglerDemo02">
          <ul
            class="navbar-nav me-auto mb-2 mb-lg-0"
            style="text-transform: capitalize"
          >
            <li class="nav-item">
              <a class="nav-link active" aria-current="page" href="home"
                >&#127968; Home</a
              >
            </li>
            <li class="nav-item">
              <a class="nav-link" href="halamanInput">barang masuk</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="barangTerjual">barang terjual</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="dataBarangMasuk">data barang masuk</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="dataBarangTerjual"
                >data barang terjual</a
              >
            </li>
          </ul>
        </div>
      </div>
    </nav>
    <!--! NAVBAR END-->

    <!--! VIEW BARANG -->
    <div class="container view mt-5">
      <h1
        class="text-center"
        style="text-transform: capitalize; margin-bottom: 15px"
      >
        data barang terjual
      </h1>
      <table class="table" id="table">
        <thead>
          <tr class="text-center">
            <th scope="col">no</th>
            <th scope="col">kode Barang</th>
            <th scope="col">nama Barang</th>
            <th scope="col">jumlah barang terjual</th>
            <th scope="col">jumlah harga terjual</th>
            <th scope="col">tanggal</th>
            <th scope="col">status</th>
          </tr>
        </thead>
        <tbody>
          <% if (barangTerjual.length == 0) { %>
          <tr>
            <td colspan="7">
              <div class="alert alert-danger" role="alert">
                <b>data contacts kosong ;</b>
              </div>
            </td>
          </tr>
          <% } else { %> <% barangTerjual.forEach((barangT,index) => { %>
          <tr class="text-center">
            <th scope="row"><%= index + 1 %></th>
            <td><%= barangT.kodeBarang %></td>
            <td><%= barangT.namaBarang %></td>
            <td><%= barangT.jumlahBarangTerjual %></td>
            <td><%= barangT.jumlahHargaTerjual %></td>
            <td><%= barangT.tanggal %></td>
            <td><a href="" class="btn btn-danger">delete</a></td>
          </tr>
          <% }) %> <% } %>
        </tbody>
      </table>
    </div>
    <!--! VIEW BARANG END-->
  </div>

  <!--! FOOTER -->
  <footer class="mt-5">
    <div class="container-fluid text-center bg-secondary">
      <div class="row">
        <div class="col-sm">
          <div class="container">
            <h5 style="text-transform: capitalize">tentang kami</h5>
            <h6>alamat :</h6>
            <p>&#127978; V97J+CQ Renged, Kabupaten Serang, Banten</p>
          </div>
        </div>
        <div class="col-sm">
          <div class="container">
            <h5 style="text-transform: capitalize">layanan</h5>
          </div>
        </div>
        <div class="col-sm">
          <div class="container">
            <h5 style="text-transform: capitalize">&#128383; hubungi</h5>
          </div>
        </div>
      </div>
    </div>
  </footer>
  <!--! FOOTER END-->
  <!-- MY SCRIPT JS -->
</body>
