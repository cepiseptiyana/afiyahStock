  <div id="form">
    <div class="h1">
      <h1>data barang terjual</h1>
    </div>
    <div class="container list w">
      <table class="table display table-striped" id="myTable">
        <thead>
          <tr>
            <th scope="col">no</th>
            <th scope="col">kode Barang</th>
            <th scope="col">nama Barang</th>
            <th scope="col">jumlah barang terjual</th>
            <th scope="col">jumlah harga terjual</th>
            <th scope="col">tanggal</th>
            <th scope="col">status</th>
          </tr>
        </thead>
        <tbody>
          <% if (barangTerjual.length == 0) { %>
          <tr>
            <th>1</th>
            <td>kosong</td>
            <td>kosong</td>
            <td>kosong</td>
            <td>kosong</td>
            <td>kosong</td>
            <td>kosong</td>
          </tr>
          <% } else { %> <% barangTerjual.forEach((data,index) => { %>
          <tr>
            <th scope="row"><%= index + 1 %></th>
            <td><%= data.id %></td>
            <td><%= data.namaBarang %></td>
            <td><%= data.jumlahBarangTerjual %></td>
            <td><%= data.jumlahHargaTerjual %></td>
            <td><%= data.tanggal %></td>
            <td>
              <a
                href="/dataBarangTerjual/delete/<%= data.id %>"
                class="btn btn-danger"
                onclick="return confirm('Yakin mennghapus data?')"
                >delete</a
              >
            </td>
          </tr>
          <% }) %> <% } %>
        </tbody>
      </table>
    </div>
  </div>
  <!--! VIEW BARANG END-->

